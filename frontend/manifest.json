{
  "manifest_version": 3,
  "name": "Smart Hint Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
  "*://*.leetcode.com/*",
  "http://localhost:3000/*"
],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.leetcode.com/*"],
      "js": ["content.js"]
    }
  ]
}